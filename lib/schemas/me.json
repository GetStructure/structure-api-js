{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "creationDate": {
      "type": "string",
      "format": "date-time"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "emailVerified": {
      "type": "boolean"
    },
    "firstName": {
      "type": "string"
    },
    "fullName": {
      "type": "string"
    },
    "id": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    },
    "lastName": {
      "type": "string"
    },
    "passwordLastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "userId": {
      "type": "string",
      "pattern": "^[A-Fa-f\\d]{24}$"
    }
  },
  "required": [
    "creationDate",
    "email",
    "emailVerified",
    "firstName",
    "fullName",
    "id",
    "lastName",
    "passwordLastUpdated",
    "userId"
  ]
}